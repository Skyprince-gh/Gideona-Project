<div class="col s12 m3">
    <div class="card" id= <%=cdata.id %>>
        <div class="card-image waves-effect waves-block waves-purple">
            <img class="activator" src=<%=cdata.document.imgURL %>>
        </div>
        <div class="card-content">
            <span class=" truncate card-title activator grey-text text-darken-4"><%=cdata.document.name%> 
                <i class="material-icons right">more_vert</i></span>
            <p><a href="#" class="btn btn-small purple" onclick="M.toast({html: `<%=cdata.document.name%> added`})">Add to Cart</a>  <strong>GHS <%=cdata.document.price%>.00</strong> </p>
        </div>
        <div class="card-reveal purple white-text">
            <span class="card-title"><%=cdata.document.name%><i class="material-icons right">close</i></span>
            <p><%=cdata.document.info%></p>
        </div>
    </div>
</div>